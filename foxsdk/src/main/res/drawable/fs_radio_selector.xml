<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="false">
        <shape>
            <size android:width="@dimen/dp_16" android:height="@dimen/dp_16" />
            <stroke android:width="@dimen/dp_1" android:color="@color/white_transparent_60" />
            <corners android:radius="@dimen/dp_8" />
        </shape>
    </item>

    <item android:state_checked="true">
        <layer-list>
            <item>
                <shape>
                    <size android:width="@dimen/dp_16" android:height="@dimen/dp_16" />
                    <stroke android:width="@dimen/dp_1" android:color="@color/fs_money" />
                    <corners android:radius="@dimen/dp_8" />
                </shape>
            </item>
            <item android:bottom="3.5dp" android:left="3.5dp" android:right="3.5dp" android:top="3.5dp">
                <shape>
                    <solid android:color="@color/fs_money" />
                    <size android:width="@dimen/dp_9" android:height="@dimen/dp_9" />
                    <corners android:radius="@dimen/dp_5" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>